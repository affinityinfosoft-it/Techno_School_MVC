@model SchoolMVC.Models.UserMaster_UM
@{
    ViewBag.Title = "User";
    Layout = "~/Views/Shared/_SchoolLayout.cshtml";
}
<!-- Bootstrap Select Css -->
<link href="~/Content/plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />
<link href="~/Content/plugins/datepicker/datepicker3.css" rel="stylesheet" />
<section class="content">
    <div class="container-fluid">
        @*<div class="block-header">
                <h2>Financial Year Master</h2>
            </div>*@
        <!-- Vertical Layout | With Floating Label -->
        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="card">
                    <div class="header">
                        <h2>
                             Create User
                            <small></small>
                        </h2>

                    </div>
                    <div class="body">
                        <form>
                            <div class="row">
                                <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                                    <div class="demo-masked-input">
                                        <div class="input-group">
                                            <div class="form-line">
                                                <label>Login Id</label>
                                                @Html.EditorFor(model => model.UM_LOGINID, new { htmlAttributes = new { @class = "form-control", required = "required", placeholder = "To Age" } })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                                    <div class="demo-masked-input">
                                        <div class="input-group">
                                            <div class="form-line">
                                                <label> PassWord</label>
                                                @Html.EditorFor(model => model.UM_PASSWORD, new { htmlAttributes = new { @class = "form-control", required = "required", placeholder = "Preference" } })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                                    <div class="demo-masked-input">
                                        <div class="input-group">
                                            <div class="form-line">
                                                <label>Confirm PassWord</label>
                                                @Html.EditorFor(model => model.UM_PASSWORD, new { htmlAttributes = new { @class = "form-control", required = "required", placeholder = "Preference" } })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                                    <div class="demo-masked-input">
                                        <div class="input-group">
                                            
                                            <label>Login Id</label>
                                            @Html.DropDownList("CM_CTM_TYPEID", null, "Select Class Type", new { @class = "form-control show-tick", required = "required" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                                    <div class="demo-masked-input">
                                        <div class="input-group">
                                            <div class="form-line">
                                                @Html.EditorFor(model => model.CM_CLASSCODE, new { htmlAttributes = new { @class = "form-control", required = "required", placeholder = "Class Code", @maxlength = "4" } })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                                    <div class="demo-masked-input">
                                        <div class="input-group">
                                            <div class="form-line">
                                                @Html.EditorFor(model => model.CM_CLASSNAME, new { htmlAttributes = new { @class = "form-control", required = "required", placeholder = "Class Name" } })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                                    <div class="demo-masked-input">
                                        <div class="input-group">
                                            <div class="form-line">
                                                @Html.EditorFor(model => model.CM_FROMAGE, new { htmlAttributes = new { @class = "form-control", required = "required", placeholder = "From Age" } })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                            <button type="button" id="btnSave" class="btn btn-danger m-t-15 waves-effect" onclick="InsertUpdateClass();">Save</button>
                            @Html.ActionLink("View List", "ClassList", "Masters", null, new { @class = "btn btn-danger m-t-15 waves-effect" })
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <!-- Vertical Layout | With Floating Label -->
    </div>
</section>
<!-- Jquery Core Js -->


