<style>
   .admission-header .card-header {
    background-color: #4450b1f0; /* green box */
    height: 50px;
    display: flex;
    align-items: center;
    padding-left: 15px;
    border-radius: 4px 4px 0 0;
    color:#e7eeeafc;

}
    /* Blur background when loader visible */
    body.loader-active section.content {
        filter: blur(4px);
        pointer-events: none;
        user-select: none;
    }

    /* Loader overlay styling */
    #loader-overlay {
        position: fixed;
        z-index: 9999;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: rgba(255,255,255,0.9) url('../../Content/images/blur-bg.jpg') center/cover no-repeat;
        display: flex;
        justify-content: center;
        align-items: center;
        flex-direction: column;
        backdrop-filter: blur(3px);
        text-align: center;
    }

    .loader-content {
        text-align: center;
        animation: fadeIn 0.5s ease-in-out;
    }

    .loader-spinner {
        border: 6px solid #f3f3f3;
        border-top: 6px solid #3498db;
        border-radius: 50%;
        width: 60px;
        height: 60px;
        animation: spin 1s linear infinite;
        margin: 15px auto;
    }

    .loader-text-top {
        font-size: 24px;
        font-weight: 600;
        color: #2c3e50;
        text-transform: uppercase;
        letter-spacing: 2px;
    }

    .loader-text-bottom {
        font-size: 18px;
        color: #555;
    }

  @@keyframes spin {
    from { transform: rotate(0deg); }
    to { transform: rotate(360deg); }
}

@@keyframes fadeIn {
    from { opacity: 0; transform: scale(0.9); }
    to { opacity: 1; transform: scale(1); }
}
</style>


