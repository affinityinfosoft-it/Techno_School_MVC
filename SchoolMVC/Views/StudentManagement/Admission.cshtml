                                                @Html.DropDownList("ddlMonth", null, "Select Month", new { @class = "form-control show-tick", required = "required" })

                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <button type="button" id="btnSave" class="btn btn-danger m-t-15 waves-effect" onclick="InsertUpdateAdmissionData();">Save</button>
                                @Html.ActionLink("View List", "AdmittedStudentList", "StudentManagement", null, new { @class = "btn btn-danger m-t-15 waves-effect" })
                            </form>
                        </div>

